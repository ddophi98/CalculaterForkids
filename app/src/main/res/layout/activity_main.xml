            android:alpha="0.0"
            app:layout_constraintEnd_toStartOf="@+id/imgSecond2"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:srcCompat="@drawable/strawberry_img" />

        <ImageView
            android:id="@+id/imgSecond2"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="32dp"
            android:alpha="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"
            app:srcCompat="@drawable/strawberry_img" />

        <ImageView
            android:id="@+id/imgSecond3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:alpha="0.0"
            app:layout_constraintEnd_toStartOf="@+id/imgSecond4"
            app:layout_constraintTop_toBottomOf="@+id/imgSecond0"
            app:srcCompat="@drawable/strawberry_img" />

        <ImageView
            android:id="@+id/imgSecond4"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:alpha="0.0"
            app:layout_constraintEnd_toStartOf="@+id/imgSecond5"
            app:layout_constraintTop_toBottomOf="@+id/imgSecond1"
            app:srcCompat="@drawable/strawberry_img" />

        <ImageView
            android:id="@+id/imgSecond5"
