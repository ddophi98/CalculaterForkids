            app:layout_constraintEnd_toStartOf="@+id/testImg2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/testImg0"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/test_img9" />

        <ImageView
            android:id="@+id/testImg2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:alpha="0.0"
            app:layout_constraintBottom_toTopOf="@+id/testImg7"
            app:layout_constraintEnd_toStartOf="@+id/testImg3"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/testImg1"
            app:layout_constraintTop_toTopOf="@+id/testImg0"
            app:srcCompat="@drawable/test_img9" />

        <ImageView
            android:id="@+id/testImg3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:alpha="0.0"
            app:layout_constraintBottom_toTopOf="@+id/testImg8"
            app:layout_constraintEnd_toEndOf="@+id/testImg8"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/testImg2"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/test_img9" />

        <ImageView
            android:id="@+id/testImg4"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="32dp"
            android:alpha="0.0"
            app:layout_constraintBottom_toTopOf="@+id/txtResult"
            app:layout_constraintEnd_toStartOf="@+id/testImg5"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/testImg1"
            app:srcCompat="@drawable/test_img9" />

        <ImageView
            android:id="@+id/testImg5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="8dp"
            android:alpha="0.0"
            app:layout_constraintBottom_toBottomOf="@+id/testImg4"
            app:layout_constraintEnd_toStartOf="@+id/testImg6"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/testImg4"
            app:layout_constraintTop_toBottomOf="@+id/testImg1"
